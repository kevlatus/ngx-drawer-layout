    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.13",
    "@types/jasmine": "~3.4.5",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.12.5",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "scss-bundle": "^3.0.1",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslint": "~5.20.0",
    "typescript": "~3.5.3"
